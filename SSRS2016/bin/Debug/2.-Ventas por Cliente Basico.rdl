  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>RetailDB</DataSourceName>
        <CommandText>SELECT ma.persona.persona, ma.persona.rucDni, ma.persona.razonSocial, ve.documento.numeroGuia, ve.documento.fechaMovimiento, ve.documento.total, ve.documento.montoIgv
FROM     ve.documento INNER JOIN
                  ma.tipoDocumento ON ve.documento.tipoDocumento = ma.tipoDocumento.tipoDocumento INNER JOIN
                  ma.persona ON ve.documento.persona = ma.persona.persona INNER JOIN
                  ma.relacionPersona ON ma.persona.persona = ma.relacionPersona.persona INNER JOIN
                  ma.tipoPersona ON ma.relacionPersona.tipoPersona = ma.tipoPersona.tipoPersona
WHERE  (ma.tipoPersona.descripcionTipoPersona = 'Clientes') AND (ma.tipoDocumento.nombreDocumento = 'BOLETA VENTA') OR
                  (ma.tipoDocumento.nombreDocumento = 'FACTURA DE VENTA')</CommandText>
      </Query>
      <Fields>
        <Field Name="persona">
          <DataField>persona</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="rucDni">
          <DataField>rucDni</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="razonSocial">
